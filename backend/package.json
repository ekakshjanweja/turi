{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --env-file-.env.local --watch src/index.ts",
    "start": "bun run src/index.ts",
    "db:gen": "bun drizzle-kit generate --config=drizzle.config.ts",
    "db:push": "bun drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "bun drizzle-kit studio",
    "db:viz": "bun run drizzle-lab visualizer",
    "deploy": "bun run cf-gen-types && wrangler secret bulk .dev.vars && wrangler deploy --minify",
    "cf-gen-types": "wrangler types --env-interface CloudflareBindings",
    "better-auth-gen-schema": "bunx @better-auth/cli@latest generate --config ./src/lib/auth.ts --output ./src/lib/db/schema/auth.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.1",
    "wrangler": "^4.20.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
    "@elevenlabs/elevenlabs-js": "^2.3.0",
    "ai": "^4.3.16",
    "better-auth": "^1.2.10",
    "dedent": "^1.6.0",
    "dotenv": "^16.5.0",
    "drizzle-lab": "^0.11.0",
    "drizzle-orm": "^0.44.2",
    "googleapis": "^150.0.1",
    "hono": "^4.8.2",
    "postgres": "^3.4.7",
    "visualizer": "^3.1.0",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.5"
  }
}
